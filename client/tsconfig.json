{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"rootDir": ".",
		"lib": ["es6", "es2017"],
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": ["src", "test"],
	"exclude": ["node_modules", ".vscode-test"]
}
